{
  "project": {
    "version": "1.0.0-beta.16",
    "name": "ng-beta-demo"
  },
  "apps": [{
    "root": "src",
    "outDir": "dist",
    "assets": "assets",
    "index": "index.html",
    "main": "main.ts",
    "test": "test.ts",
    "tsconfig": "tsconfig.json",
    "prefix": "app",
    "mobile": false,
    "styles": [
      "../node_modules/bootstrap/dist/css/bootstrap.min.css",
      "../node_modules/font-awesome/css/font-awesome.min.css",
      "assets/css/plugins/simple-line-icons.css",
      "../node_modules/animate.css/animate.min.css",
      "assets/css/plugins/datatables.bootstrap.min.css",
      "styles.css"
    ],
    "scripts": [
      "../node_modules/jquery/dist/jquery.min.js",
      "../node_modules/tether/dist/js/tether.min.js",
      "../node_modules/bootstrap/dist/js/bootstrap.min.js",
      "assets/js/plugins/moment.min.js",
      "assets/js/plugins/fullcalendar.min.js",
      "assets/js/plugins/jquery.nicescroll.js",
      "assets/js/plugins/jquery.vmap.min.js",
      "assets/js/plugins/maps/jquery.vmap.world.js",
      "assets/js/plugins/jquery.vmap.sampledata.js",
      "assets/js/plugins/chart.min.js",
      "assets/js/plugins/jquery.datatables.min.js",
      "assets/js/plugins/datatables.bootstrap.min.js",
      "assets/js/main.js"
    ],
    "environments": {
      "source": "environments/environment.ts",
      "dev": "environments/environment.ts",
      "prod": "environments/environment.prod.ts"
    }
  }],
  "addons": [],
  "packages": [],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "prefixInterfaces": false,
    "lazyRoutePrefix": "+"
  }
}
